                                    $sengundo_apellido=$array_nombre[1];
                                    $profesor->setApellido($primer_apellido);
                                    $profesor->setApellido1($sengundo_apellido);
                                    $profesor->setNombre($primer_nombre);   
                   

                                }
                                if(count($array_nombre)==3){
                                    $primer_nombre=$array_nombre[1];                            
