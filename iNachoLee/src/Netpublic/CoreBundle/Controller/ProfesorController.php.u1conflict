                     } 
                     $em->persist($usuario);                                                                               
                     $em->flush();
                     $usuarios[]=$usuario;                                                                                    
                      }
                     $fila++;
                 }
             }  
             
                $em->flush();
                
             return array(
                 'usuarios'=>$usuarios
             );   

    }

    
