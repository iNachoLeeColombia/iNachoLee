			 if($entity->getTipo()==4){
                //Rector
                $mi_rol=  $this->getDoctrine()->getRepository("NetpublicCoreBundle:Rol")->find(3);
            }
			 if($entity->getTipo()==5){
                //Rector
                $mi_rol=  $this->getDoctrine()->getRepository("NetpublicCoreBundle:Rol")->find(3);
            }
			 if($entity->getTipo()==6){
                //Rector
                $mi_rol=  $this->getDoctrine()->getRepository("NetpublicCoreBundle:Rol")->find(3);
            }
	
            $usuario=$entity->getUsuario();
	    $roles_usuario=$usuario->getUserRol();	
