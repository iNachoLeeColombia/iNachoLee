<h1>HorarioClase</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Hora_inicio</th>
            <td>{{ entity.horainicio }}</td>
        </tr>
        <tr>
            <th>Hora_final</th>
            <td>{{ entity.horafinal }}</td>
        </tr>
        <tr>
            <th>Dia_semana</th>
            <td>{{ entity.diasemana }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('horarioclase') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('horarioclase_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('horarioclase_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
