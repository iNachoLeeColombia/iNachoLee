
<div class="col-md-12">
{%if sedes!=0%}
<div class="form-group col-xs-3">
    <select class="form-control" id="{{sede_id_js}}"
        onchange="procesarFiltrosGeneral(this,'{{id_capa}}','{{sede_id_js}}','{{grado_id_js}}','{{grupo_id_js}}','{{url}}');">
    <option value="*">Sede</option>
    {%for sede in sedes%} 
    <option value="{{sede.id}}">{{sede}}</option>
   {%endfor%} 
</select>
</div>        
{%endif%}
<div class="form-group col-xs-4">
    <select class="form-control" id="{{grado_id_js}}"
        onchange="procesarFiltrosGeneral(this,'{{id_capa}}','{{sede_id_js}}','{{grado_id_js}}','{{grupo_id_js}}','{{url}}');getGruposGradoEstandar('{{grado_id_js}}','{{grupo_id_js}}');">
    <option value="*">Grado</option>
   {%for grado in grados%} 
    <option value="{{grado.id}}">{{grado}}</option>
   {%endfor%} 
</select>
</div>
<div class="form-group col-xs-5">

    <select class="form-control" id="{{grupo_id_js}}" 
        onchange="procesarFiltrosGeneral(this,'{{id_capa}}','{{sede_id_js}}','{{grado_id_js}}','{{grupo_id_js}}','{{url}}');">
    <option value="*" >Grupo</option>
</select>
</div>        
</div>        