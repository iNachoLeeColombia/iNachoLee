<h1>Matricula</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Es_matricula</th>
            <td>{{ entity.esmatricula }}</td>
        </tr>
        <tr>
            <th>Es_pago_matricula</th>
            <td>{{ entity.espagomatricula }}</td>
        </tr>
        <tr>
            <th>Es_papeles</th>
            <td>{{ entity.espapeles }}</td>
        </tr>
        <tr>
            <th>Observaciones</th>
            <td>{{ entity.observaciones }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('matricula') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('matricula_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('matricula_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
