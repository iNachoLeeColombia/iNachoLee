<form action="{{ path('profesor_create') }}" method="post" {{ form_enctype(form) }}>
  <div id="accordion">  
      <div class="col-lg-12">
        <div id="new_colegio_ano_escolar" > 
        <div>
            <div class="col-lg-4">  
                <div class="form-group">
                {{form_label(form.nombre)}}
                {{ form_widget(form.nombre)}}         
                </div>
                <div class="form-group">    
                {{form_label(form.nombre1)}}
                {{ form_widget(form.nombre1)}}       
                </div>
                <div class="form-group">
                {{form_label(form.apellido)}}
                {{ form_widget(form.apellido)}}       
                </div>
                <div class="form-group">    
                {{form_label(form.apellido1)}}
                {{ form_widget(form.apellido1)}}               
                </div>
                <div class="form-group">
                {{form_label(form.tipo_documento)}}
                {{ form_widget(form.tipo_documento)}}       
                </div>
                <div class="form-group">
                {{form_label(form.tipo)}}
                {{form_widget(form.tipo)}}       
                </div>
                <div class="form-group">
                {{form_label(form.cedula)}}
                {{form_widget(form.cedula)}}<span id="notificador_cedula"></span>        
                </div>
                <div class="form-group">
                {{form_label(form.sede)}}
                {{form_widget(form.sede)}} 
                </div>        
           </div>
           <div class="col-lg-2">
           </div>     
                <div class="col-lg-4">
                {{form_label(form.genero)}}
                {{ form_widget(form.genero)}}         
                {{form_label(form.fecha_nacimiento)}}
                {{form_widget(form.fecha_nacimiento)}} 
                {{form_label(form.nivel_educativo_aprobado)}}
                {{form_widget(form.nivel_educativo_aprobado)}} 
                {{form_label(form.tipo_vinculacion)}}
                {{form_widget(form.tipo_vinculacion)}}              
                {{form_label(form.foto_academica)}}
                {{form_widget(form.foto_academica)}} 
                {{form_label(form.foto_firma)}}
                {{form_widget(form.foto_firma)}} 
                {{form_widget(form._token)}} 
             </div>                            
        </div>  
      </div>
 </div>
          <div class="col-lg-4"></div>
          <div class="col-lg-4"></div>         
 <div class="col-lg-4 pull-right">
       <button type="submit" class="btn btn-primary" disabled="" id="boton_guardar_profesor">Guardar Nuevo Profesor </button>
 </div> 
 </div>  
</form>
