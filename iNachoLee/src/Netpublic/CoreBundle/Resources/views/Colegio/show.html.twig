{% extends 'NetpublicCoreBundle::layout.html.twig' %}
{% block title %}Calificar Grupo{% endblock %}
{%block  sub_container  %} 

<h1>Colegio</h1>

<table class="bordered-table zebra-striped">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Codigo_dane</th>
            <td>{{ entity.codigodane }}</td>
        </tr>
        <tr>
            <th>Numero_sedes</th>
            <td>{{ entity.numerosedes }}</td>
        </tr>
        <tr>
            <th>Es_subsidio</th>
            <td>{{ entity.essubsidio }}</td>
        </tr>
        <tr>
            <th>Barrio</th>
            <td>{{ entity.barrio }}</td>
        </tr>
        <tr>
            <th>Direccion</th>
            <td>{{ entity.direccion }}</td>
        </tr>
        <tr>
            <th>Telefono</th>
            <td>{{ entity.telefono }}</td>
        </tr>
        <tr>
            <th>Fax</th>
            <td>{{ entity.fax }}</td>
        </tr>
        <tr>
            <th>Web</th>
            <td>{{ entity.web }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Es_principal</th>
            <td>{{ entity.esprincipal }}</td>
        </tr>
        <tr>
            <th>Numero_linc_func</th>
            <td>{{ entity.numerolincfunc }}</td>
        </tr>
    </tbody>
</table>

        <a class="btn small" href="{{ path('colegio') }}">
            Back to the list
        </a>
   
        <a class="btn small"  href="{{ path('colegio_edit', { 'id': entity.id }) }}">
            Edit
        </a>
  
        <form action="{{ path('colegio_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button class="btn small"  type="submit">Delete</button>
        </form>
  <form action="{{ path('colegio_newsede')}}" method="post">
            {{ form_widget(delete_form) }}
          <button style="float: right" class="ui-button-primary"value="{{ entity.numerosedes }}" type="submit">Siguiente >></button>

        </form>

{% endblock %}
