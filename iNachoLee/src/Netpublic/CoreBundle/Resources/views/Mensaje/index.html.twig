<h5>Bandejax De Entrada</h5>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Remitente</th>
            <th>Asunto</th>            
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('mensaje_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.cuerpohtml }}</td>
            <td>{{ entity.tipo }}</td>
            
            <td>{{ entity.urldocadjunto }}</td>
            <td>{{ entity.asunto }}</td>
           
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="#" onclick="newMensajeinfoAcademicaDefectoAno();">
            Redactar.
        </a>
    </li>
</ul>
